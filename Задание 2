#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;
int binnary(unsigned int a, unsigned int b) {
	while (b != 0) {
		unsigned int carry = a & b;
		a = a ^ b;
		b = carry << 1;
	}
	return a;
}
void print_num(unsigned int n) {
    if (n == 0) {
        cout << "0";
        return;
    }
    int bits[32];
    int index = 0;
    while (n > 0) {
        bits[index++] = n % 2;
        n /= 2;
    }
    for (int i = index - 1; i >= 0; i--) {
        cout << bits[i];
    }
}
int main() {
	setlocale(LC_ALL, "Russian");
	srand(static_cast<unsigned int>(time(NULL)));
    unsigned int a = rand() % 64;
    unsigned int b = rand() % 64;
    cout << "число 1 " <<"\n";
    print_num(a);
    cout << "\n";
    cout << "число 2"<< "\n";
    print_num(b);
    unsigned int res = binnary(a, b);
    cout << "\n";
    cout << "результат" << endl;
    print_num(res);
    return 0;

}
